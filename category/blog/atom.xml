    <content type="html"><![CDATA[<script type="text/javascript">
  
 
  
        var a = "HappyBirthday Bao" ;
        var b = 0 ; 
setTimeout("type()", 1000) ;

        function type() { 
            document.getElementById("hbbao").innerHTML  = a.substring(0, b);
            if (b == a.length) { stop;} 
            else{ b++ ;}
            setTimeout("type()", 300) ;
        }
 
</script>


